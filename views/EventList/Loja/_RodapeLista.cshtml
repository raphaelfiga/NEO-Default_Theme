@model List<DomainCustomer.Entities.EventListType>
@if (Model != null && Model.Count > 0)
{
    <h4 class="ui header">LISTA DE EVENTOS</h4>
    <div class="ui link list">
        @if(Model != null) { 
        foreach (var item in Model)
            {
                <a href="/EventList/SearchList/?type=@item.IdType" class="item">@item.Title</a>
            }
        }
    </div>
}
